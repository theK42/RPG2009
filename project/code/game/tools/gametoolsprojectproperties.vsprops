	ProjectType="Visual C++"
	Version="8.00"
	Name="gametoolsprojectproperties"
	OutputDirectory="$(SolutionDir)$(ConfigurationName)\game\tools"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(SolutionDir)&quot;;&quot;$(SolutionDir)..\target\mswin\include&quot;"
